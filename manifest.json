{
  "manifest_version": 2,
  "name": "Breezy's New Tab Page",
  "version": "1.0",
  "description": "Replaces Google's new tab page with my own custom page",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}
